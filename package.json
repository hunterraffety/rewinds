{
  "name": "rewinds",
  "description": "Remix Tailwind Starter Kit with Tailwind CSS family of libraries",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "start": "doppler run -- npm run dev",
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -w -i ./styles/global.css -o app/styles/out.css",
    "build": "npm-run-all build:*",
    "build:css": "tailwindcss -m -i ./styles/global.css -o app/styles/out.css",
    "build:remix": "remix build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@balavishnuvj/remix-seo": "1.0.2",
    "@headlessui/react": "1.6.1",
    "@heroicons/react": "1.0.6",
    "@june-so/analytics-next": "1.36.3",
    "@radix-ui/react-collapsible": "0.1.6",
    "@radix-ui/react-context-menu": "0.1.6",
    "@radix-ui/react-dropdown-menu": "0.1.6",
    "@radix-ui/react-navigation-menu": "0.1.2",
    "@radix-ui/react-scroll-area": "0.1.4",
    "@radix-ui/react-tabs": "0.1.5",
    "@remix-run/node": "1.4.3",
    "@remix-run/react": "1.4.3",
    "@remix-run/vercel": "1.4.3",
    "@splitbee/web": "0.3.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.1",
    "@tailwindcss/line-clamp": "0.4.0",
    "@tailwindcss/typography": "0.5.2",
    "@tanem/react-nprogress": "5.0.0",
    "@urql/core": "2.4.4",
    "@vechaiui/core": "0.6.0",
    "@vechaiui/react": "0.2.2",
    "@vercel/node": "1.15.0",
    "axios": "0.27.2",
    "clsx": "1.1.1",
    "daisyui": "2.14.3",
    "date-fns": "2.28.0",
    "dayjs": "1.11.1",
    "graphql": "16.4.0",
    "graphql-request": "4.2.0",
    "phosphor-react": "1.4.1",
    "posthog-js": "1.20.4",
    "posthog-node": "1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga4": "1.4.1",
    "react-hook-form": "7.30.0",
    "react-icons": "4.3.1",
    "react-router-dom": "6.3.0",
    "remix": "1.4.3",
    "remix-utils": "3.1.0",
    "tailwindcss": "3.0.24",
    "tailwindcss-radix": "2.2.0",
    "usehooks-ts": "2.5.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.4.3",
    "@remix-run/eslint-config": "1.4.3",
    "@remix-run/serve": "1.4.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "10.4.7",
    "eslint": "8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
